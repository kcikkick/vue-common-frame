{
  "name": "basic-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "set NODE_ENV=development&&nodemon ./build/dev-server.js",
    "build": "set NODE_ENV=production&&nodemon ./build/prod-server.js",
    "org-serve": "vue-cli-service serve --mode development --port 7779",
    "org-build": "vue-cli-service build --mode production --port 7779",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "core-js": "^2.6.5",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "graceful": "^1.0.2",
    "http-proxy-middleware": "^0.19.1",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.11",
    "shelljs": "^0.8.3",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.21",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/no-use-v-if-with-v-for": "warning",
      "vue/html-quotes": "error",
      "vue/this-in-template": "error",
      "vue/arrow-spacing": "error"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
